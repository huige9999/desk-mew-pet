{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "desk-mew-pet_demo",
  "version": "0.1.0",
  "identifier": "com.vichel.deskmewpetdemo",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "desk-mew-pet_demo",
        "width": 360,
        "height": 280,
        "resizable": false,
        "fullscreen": false,
        "alwaysOnTop": true,
        "decorations": false,
        "transparent": true,
        "shadow": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
